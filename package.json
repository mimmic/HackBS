{
  "name": "hack-bs",
  "version": "0.0.1",
  "scripts": {
    "test": "webpack serve --config webpack/test.config.js --mode development --progress --hot",
    "auto-test": "ava",
    "eslint": "./node_modules/.bin/eslint --ext .js,.vue src",
    "dev": "webpack serve --config webpack/dev.config.js --mode development --progress --hot",
    "ww": "webpack serve --config webpack/dev.config.js --mode production --progress --hot",
    "build": "webpack  --config webpack/build.config.js --mode production --progress",
    "cdn": "webpack serve --config webpack/cdn.config.js --mode development --progress --hot",
    "deploy": "sh deploy.sh"
  },
  "ava": {
    "babel": {
      "compileAsTests": [
        "test/auto_tests/**/*",
        "src/**/*"
      ]
    },
    "files": [
      "test/auto_tests/**/*"
    ]
  },
  "author": "mimmic",
  "files": [
    "dist/*",
    "src/*"
  ],
  "keywords": [
    "trading",
    "vue",
    "charts",
    "crypto",
    "visualization",
    "reactive",
    "algo-trading",
    "finviz",
    "trading-systems"
  ],
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-parameters": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "ava": "^3.15.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.21.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-plugin-vue": "^7.7.0",
    "html-webpack-plugin": "^5.3.2",
    "null-loader": "^4.0.1",
    "raw-loader": "^4.0.2",
    "request": "^2.88.2",
    "terser-webpack-plugin": "^5.1.1",
    "tulind": "^0.8.18",
    "vue-codemirror": "^4.0.6",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "arrayslicer": "^1.2.3",
    "hammerjs": "^2.0.8",
    "hamsterjs": "^1.1.3",
    "hotkeys-js": "^3.8.7",
    "lz-string": "^1.4.4",
    "moment": "^2.29.1",
